alias get-daemon="kubectl get pods -l component=ws-daemon"
alias get-manager="kubectl get pods -l component=ws-manager"
alias get-ws="kubectl get pods -l workspaceType=regular"
alias get-prebuild="kubectl get pods -l workspaceType=prebuild"
alias get-smith="kubectl get pods -l component=agent-smith"
alias get-reg="kubectl get pods -l component=registry-facade"
alias get-proxy="kubectl get pods -l component=ws-proxy"
alias get-blob="kubectl get pods -l component=blobserve"